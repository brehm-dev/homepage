html
    head
        meta(charset="utf-8")
        meta(name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no, user-scalable=no")
        link(rel="shortcut icon" type="image/x-icon" href="images/1512913768.ico")
        title Andreas Brehm
        link(rel="stylesheet" href="css/style.css")
    body
        header
            nav.ab-navbar
                .ab-navbar-menu-bar
                    .ab-navbar-menu-brand
                        img.ab-brand(src="images/my_logo_trans.png")
                    .ab-navbar-list
                        .ab-navbar-group
                            .ab-navbar-group-item#previous
                                .ab-navbar-link
                                    span bisherige Arbeit
                            .ab-navbar-group-item#aims
                                .ab-navbar-link
                                    span berufliche Ziele
                            .ab-navbar-group-item#about
                                .ab-navbar-link
                                    span Über Mich
                    .ab-navbar-menu-button-bar
                        .ab-navbar-menu-button#menu-button(data-activity="false")
                            .ab-navbar-menu-border
                                .ab-navbar-menu-item
                                .ab-navbar-menu-item
                                .ab-navbar-menu-item

        section
            .ab-cardsite
                .ab-cardsite-banner
                    .ab-cardsite-layer
                        .ab-cardsite-container
                            .ab-cardsite-logo-container
                                img.ab-cardsite-logo(src="images/my_logo_trans.png")
                            .ab-cardsite-title-container
                                span.ab-cardsite-title-name Andreas Brehm
                                span.ab-cardsite-title-profession Fachinformatiker seit 2014
                                span.ab-cardsite-title-specialization Anwendungsentwicklung


        section
            .container
                h1 meine Interessen
        section
            .container
                h1 Über mich
        footer
            .container
                h1 Footer
        script(src="jquery/jquery.js")
        script(src="js/workflow.js")